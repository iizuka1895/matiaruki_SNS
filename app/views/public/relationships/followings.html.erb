<h2>Follow Users</h2>
<% if @users.exists? %>
  <% @users.each do |user| %>
    <%= image_tag user.get_profile_image(50,50) %>
    <%= user.name %>
    <p>フォロー数:</p> <%= user.followings.count %>
    <p>フォロワー数:</p> <%= user.followers.count %>
    <%= render "public/relationships/btn", user: user, options: { class: "follow-link" } %>
  <% end %>
<% else %>
  <p>ユーザーはいません</p>
<% end %>